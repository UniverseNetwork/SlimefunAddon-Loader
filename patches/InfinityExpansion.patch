+
+import java.util.ArrayList;
+import java.util.List;
+
+@SuppressWarnings("unused")
+public final class GiveRecipe {
+    @Command("infinityexpansion|ie|ix|infinity giverecipe <id>")
+    @Permission("infinityexpansion.giverecipe")
+    @CommandDescription("Gives all the items in a Slimefun item recipe")
+    public void cmdGiveRecipe(final SpecialCommandSender sender,
+                              final @Argument(value = "id", suggestions = "idList") String id) {
+        if (!sender.isPlayer()) {
+            sender.getSender().sendMessage("Only players can use this!");
+            return;
+        }
+
+        SlimefunItem sfItem = SlimefunItem.getById(id.toUpperCase());
+
+        if (sfItem == null || sfItem instanceof MultiBlockMachine || sfItem.getRecipeType() == RecipeType.GEO_MINER) {
+            sender.getSender().sendMessage(ChatColor.RED + "Invalid Slimefun item!");
+            return;
+        }
+
+        sender.getSender().sendMessage(ChatColor.GREEN + "Gave recipe for " + sfItem.getItemName());
+
+        List<ItemStack> recipe = new ArrayList<>();
+
+        for (ItemStack e : sfItem.getRecipe()) if (e != null) recipe.add(e);
+
+        sender.getPlayer().getInventory().addItem(recipe.toArray(new ItemStack[0]));
+    }
+
+    @Suggestions("idList")
+    public List<String> idList(CommandContext<SpecialCommandSender> sender, String context) {
+        List<String> tabs = new ArrayList<>();
+        for (SlimefunItem item : Slimefun.getRegistry().getEnabledSlimefunItems()) tabs.add(item.getId());
+        return tabs;
+    }
+}
